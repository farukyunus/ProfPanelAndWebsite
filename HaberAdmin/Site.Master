        $("#CikisYap").click(function () {
            $.ajax({
                url: '/ajax/AjxCikisYap.aspx/CikisYap',
                dataType: 'json',
                type: 'POST',
                data:  "{'islem': 'cikisyaptir'}",
                contentType: 'application/json; charset=utf-8',
                success: function (data) {
                    alert(data.d);
                    location.href = "/PanelGiris/Giris.aspx";
                }
            });
        });
    });
